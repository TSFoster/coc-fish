{
  "name": "coc-fish",
  "version": "1.0.6",
  "description": "Coc completion source for fish shell",
  "main": "index.js",
  "engines": {
    "coc": ">=0.0.57"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "coc.source.fish.enable": {
          "type": "boolean",
          "default": true
        },
        "coc.source.fish.shortcut": {
          "type": "string",
          "default": "fish"
        },
        "coc.source.fish.triggerCharacters": {
          "type": "array",
          "default": [
            "."
          ],
          "items": {
            "type": "string"
          }
        },
        "coc.source.fish.priority": {
          "type": "integer",
          "default": 7
        },
        "coc.source.fish.filetypes": {
          "type": [
            "array",
            "null"
          ],
          "default": [
            "fish"
          ],
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack"
  },
  "author": "",
  "license": "ISC",
  "keywords": [
    "coc.nvim",
    "neovim",
    "fish",
    "shell"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "coc.nvim": "^0.0.73",
    "eslint": "^6.0.1"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "parser": "babel-eslint",
    "rules": {
      "no-unused-vars": "warn"
    }
  }
}
